<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList> 
  
  <path>switch.ac</path>
  
  <animation>
    <object-name>plinth</object-name>
    <object-name>switch_body</object-name>
    <object-name>switch</object-name>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>switch</object-name>
    <property>controls/engines/engine[1]/magnetos</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> -45 </dep></entry>
      <entry><ind> 1 </ind><dep>   0 </dep></entry>
      <entry><ind> 2 </ind><dep>   0 </dep></entry>
    </interpolation>
    <center> 
      <x-m> 0.02115 </x-m>
      <y-m> 0.00000 </y-m>
      <z-m> 0.00000 </z-m>
    </center>
    <axis> 
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
